      "source": [
        "df = pd.read_csv('../data/processed/spam_preprocessed.csv')\n",
        "\n",
        "print(f'Dataset shape: {df.shape}')\n",
        "print(f'Columns: {df.columns.tolist()}')\n",
        "\n",
